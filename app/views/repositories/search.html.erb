<h1>Search Github Repositories</h1>
<%= form_tag '/search' do %>
  <%= label_tag :query %>
  <%= text_field_tag :query %>
  <%= submit_tag "Search Github!" %>
<% end %>
<% if @resp %>
		<% @resp.each do |item| %>
			<%= item['name'] %><br>
			<%= item['description'] %><br>
			<a id="<%=item['html_url']%>" href="<%=item['html_url']%>"><%=item['html_url'] %></a>
		<% end %>
<% end %>
