<h1>Search Github Repositories</h1>
<%= form_tag '/search' do %>
  <%= label_tag :query %>
  <%= text_field_tag :query %>
  <%= submit_tag "Search Github!" %>
<% end %>
<% if @venues %>
  <%@venues.each do |v|%>
    <li>
      <%= v["name"]%>
      <%= link_to v["html_url"], v["html_url"]  %>
      <%= v["description"]%>
    </li>
  <%end%>
<%end%>

