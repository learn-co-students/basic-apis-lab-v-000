<h1>Search Github Repositories</h1>
<%= form_tag '/search' do %>
  <%= label_tag :query %>
  <%= text_field_tag :query %>
  <%= submit_tag "Search Github!" %>

  <% if @venues
      @venues.each do |venue| %>
        <%= venue["name"]%>
        <%= venue["description"]%>
        <%= link_to venue["html_url"], venue["html_url"]%>
    <% end %>
  <% end %>

<% end %>

